<?xml version="1.0" encoding="utf-8"?>
<resources>

    <array name="ViewList">
        <item>@style/Google</item>
        <item>@style/LaguWajib</item>
        <item>@style/Bungaku</item>
    </array>

    <style name="Google">
        <item name="title">Google</item>
        <item name="icon">@drawable/ic_bookmark_on</item>
        <item name="viewClass">.view.WebBrowser</item>
        <item name="url">https://www.google.com</item>
        <item name="visible">true</item>
    </style>

    <style name="LaguWajib">
        <item name="title">Lagu Wajib</item>
        <item name="icon">@drawable/ic_bookmark_on</item>
        <item name="viewClass">.view.WebBrowser</item>
        <item name="url">zip://index.html</item>
        <item name="zip">@raw/laguwajib</item>
        <item name="visible">true</item>
    </style>

    <style name="Bungaku">
        <item name="title">Bungaku</item>
        <item name="icon">@drawable/ic_bookmark_on</item>
        <item name="viewClass">.view.WebBrowser</item>
        <item name="url">zip://bungaku.html</item>
        <item name="zip">@raw/laguwajib</item>
        <item name="visible">true</item>
    </style>

</resources>